{
  "name": "auto_recommended_tags",
  "description": "This module hooks with Apache Stanbol via a web socket to provide real-time tags recommendations when adding/editing content.",
  "license": "GPL-2.0",
  "private": true,
  "engines": {
    "yarn": ">= 1.6",
    "node": ">= 12.0"
  },
  "keywords": [
    "Drupal",
    "admin",
    "Apache Stanbol",
    "tags",
    "auto recommendation"
  ],
  "author": "Vardot",
  "scripts": {
    "phpcs": "phpcs --standard=./.phpcs.xml .",
    "phpcbf": "phpcbf --standard=./.phpcs.xml .",
    "lint:yaml": "node ./node_modules/eslint/bin/eslint.js --config=.eslintrc.json --ext .yml .",
    "lint:js": "node ./node_modules/eslint/bin/eslint.js --config=.eslintrc.json .",
    "lint:css": "npx stylelint --config=.stylelintrc.json .",
    "theme:init": "gulp",
    "theme:build": "gulp compile",
    "theme:watch": "gulp watch"
  },
  "dependencies": {  },
  "devDependencies": { 
    "autoprefixer": "~9",
    "babel-core": "~6",
