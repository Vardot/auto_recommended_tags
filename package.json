    "browser-sync": "~2",
    "chalk": "~2",
    "chokidar": "~2",
    "chromedriver": "^75.1.0",
    "cross-env": "~5",
    "del": "~3",
    "dotenv-safe": "~5",
    "eslint": "~7",
    "eslint-config-airbnb": "~18",
    "eslint-config-prettier": "~6",
    "eslint-plugin-import": "~2",
    "eslint-plugin-jquery": "~1",
    "eslint-plugin-jsx-a11y": "~6",
    "eslint-plugin-prettier": "~3",
    "eslint-plugin-react": "~7",
    "eslint-plugin-react-hooks": "~4",
    "eslint-plugin-yml": "^0.10.0",
    "glob": "^7.1.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-clean-css": "3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-csscomb": "^3.1.0",
    "gulp-filter": "^5.0.0",
    "gulp-html-replace": "^1.6.2",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
